{
	"cookie": 
	{
		"description": "Allow to give a number of cookies at a user",
		"permissions": ["write", "read"],
		"arguments": 
		[
			{
				"type": "integer",
				"matches": "0..150",
				"keep_value": true,
				"arguments": 
				[
					{
						"type": "string",
						"matches": "theo",
						"execute": "giveCookie"
					},

					{
						"type": "string",
						"matches": "leirof",
						"execute": "giveCookieAll"
					}
				]
			}
		]
	}
}