{
	"config":
	{
		"description": "Manage the bot's configuration",
		"permissions": ["discord.administrator"],
		"arguments":
		[
			{
				"tag": "list",
				"type": "string",
				"matches": "list",
				"execute": "list",
				"arguments":
				[{
					"tag": "path",
					"type": "string",
					"keep_value": true,
					"execute": "listNode"
				}]
			},
			{
				"tag": "get",
				"type": "string",
				"matches": "get",
				"arguments":
				[{
					"tag": "path",
					"type": "string",
					"keep_value": true,
					"execute": "get"
				}]
			},
			{
				"tag": "set",
				"type": "string",
				"matches": "set",
				"arguments":
				[{
					"tag": "path",
					"type": "string",
					"keep_value": true,
					"arguments":
					[{
						"tag": "value",
						"type": "string...",
						"keep_value": true,
						"execute": "set"
					}]
				}]
			},
			{
				"tag": "reset",
				"type": "string",
				"matches": "reset",
				"arguments":
				[{
					"tag": "path",
					"type": "string",
					"keep_value": true,
					"execute": "reset"
				}]
			}
		]
	}
}