{
	"permission":
	{
		"description": "Manage bot permissions",
		"permissions": ["discord.administrator", "discord.manage_guild"],
		"aliases": ["perm", "perms"],
		"arguments":
		[
			{
				"tag":"list",
				"type": "string",
				"matches": "list",
				"execute": "list"
			},
			{
				"tag":"get",
				"type": "string",
				"matches": "get",
				"arguments":
				[
					{
						"tag":"user",
						"type": "string",
						"keep_value": true,
						"execute": "getUser"
					},
					{
						"tag":"role",
						"type": "string",
						"keep_value": true,
						"execute": "getRole"
					}
				]
			},
			{
				"tag":"set",
				"type": "string",
				"matches": "set",
				"arguments":
				[{
					"tag":"permission",
					"type": "list<string>",
					"matches": "([a-z_]+\\.)+([a-z_]+|\\*)",
					"keep_value": true,
					"arguments":
					[{
						"tag":"value",
						"type": "boolean",
						"keep_value": true,
						"arguments":
						[
							{
								"tag":"users",
								"type": "list<string>",
								"keep_value": true,
								"execute": "setUsers"
							},
							{
								"tag":"roles",
								"type": "list<string>",
								"keep_value": true,
								"execute": "setRoles"
							}
						]
					}]
				}]
			}
		]
	}
}