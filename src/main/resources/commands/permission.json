{
	"permission":
	{
		"description": "Manage bot permissions",
		"permissions": ["discord.administrator", "discord.manage_guild"],
		"aliases": ["perm", "perms"],
		"arguments":
		[
			{
				"tag":"list",
				"type": "string",
				"matches": "list",
				"execute": "list"
			},
			{
				"tag":"help",
				"type": "string",
				"matches": "help|h|\\?",
				"execute": "help"
			},
			{
				"tag":"set",
				"type": "string",
				"matches": "set",
				"arguments":
				[{
					"tag":"permission",
					"type": "string",
					"matches": "custom\\.([a-z_]+\\.)*([a-z_]+|\\*)",
					"keep_value": true,
					"arguments":
					[{
						"tag":"value",
						"type": "boolean",
						"matches":"",
						"keep_value": true,
						"arguments":
						[
							{
								"tag":"users",
								"type": "string",
								"matches": "(<@!?[0-9]+>;)*<@!?[0-9]+>",
								"keep_value": true,
								"execute": "setUsers"
							},
							{
								"tag":"roles",
								"comment": "(Un)grant specified permission to specified roles",
								"type": "string",
								"matches": "(<@\\&[0-9]+>;)*<@\\&[0-9]+>",
								"keep_value": true,
								"execute": "setRoles"
							}
						]
					}]
				}]
			},
			{
				"tag":"get",
				"type": "string",
				"matches": "get",
				"arguments":
				[
					{
						"tag":"user",
						"type": "string",
						"matches": "<@!?[0-9]+>",
						"keep_value": true,
						"execute": "getUser"
					},
					{
						"tag":"role",
						"type": "string",
						"matches": "<@\\&[0-9]+>",
						"keep_value": true,
						"execute": "getRole"
					}
				]
			}
		]
	}
}