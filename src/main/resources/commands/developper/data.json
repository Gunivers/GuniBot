{
	"data": 
	{
		"description": "Display internal data of the guild/user/system in JSON format",
		"permissions": ["bot.dev"],
		"arguments":
		[
			{
				"type": "string",
				"tag": "guild",
				"matches": "guild",
				"execute": "displayGuildData"
			},
			{
				"type": "string",
				"tag": "user",
				"matches": "user",
				"arguments": 
				[{
					"type": "string",
					"tag": "user",
					"keep_value": true,
					"execute": "displayUserData"
				}]
			},
			{
				"type": "string",
				"tag": "system",
				"matches": "system",
				"arguments" :
				[{
					"type": "string",
					"tag": "system_id",
					"keep_value": true,
					"execute": "displaySystemData"
				}]
			}
		]
	}
}